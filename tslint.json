{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "no-submodule-imports": [
      true,
      "vscode"
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always",
        "method": "never",
        "constructor": "never"
      }
    ],
    "object-literal-sort-keys": true,
    "no-string-literal": true,
    "ordered-imports": true,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-console": [
      true,
      "log",
      "debug",
      "info",
      "warn"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ]
  }
}
