{
  "name": "nsri",
  "version": "6.1.0",
  "description": "Node.js utility tool for creating and checking subresource integrity",
  "license": "MIT",
  "author": {
    "email": "jimikar@gmail.com",
    "name": "Jimi (Dimitris) Charalampidis"
  },
  "repository": {
    "type": "git",
    "url": "JimiC/nsri"
  },
  "bugs": {
    "url": "https://github.com/JimiC/nsri/issues"
  },
  "homepage": "https://github.com/JimiC/nsri#readme",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "node",
    "nodejs",
    "cli",
    "integrity",
    "checker",
    "check",
    "validation",
    "file",
    "files",
    "folder",
    "forders",
    "directory",
    "directories",
    "structure",
    "hash",
    "checksum",
    "checksums",
    "nsri"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "bin": {
    "nsri": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "types": "lib/nsri.d.ts",
  "scripts": {
    "reinstall": "rimraf ./yarn.lock ./package-lock.json ./node_modules && npm i",
    "start": "node ./out/src/cli.js",
    "lint": "eslint --ext .ts .",
    "prebuild:dev": "npm run lint",
    "build:dev": "npm run build -- -p tsconfig.dev.json",
    "prebuild": "npm run cleanup",
    "build": "tsc",
    "cleanup": "rimraf ./.nyc_output ./coverage ./out ./lib ./dist",
    "pretest": "npm run build:dev",
    "test": "nyc mocha",
    "posttest": "nyc report -r lcov",
    "prepublishOnly": "npm run build && npx api-extractor run && npx rollup -c"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "cosmiconfig": "^7.0.1",
    "detect-indent": "^6.1.0",
    "minimatch": "^5.0.1",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.23.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/chai": "^4.3.1",
    "@types/minimatch": "^3.0.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "14",
    "@types/sinon": "^10.0.11",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "builtin-modules": "^3.2.0",
    "chai": "^4.3.6",
    "eslint": "^8.14.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^13.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  }
}
